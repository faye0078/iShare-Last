<!--绑定输入事件-->
<input style="border:1px solid black;" bindinput="getsuggest" value="{{backfill}}"></input>
<!--关键词输入提示列表渲染-->
<view class="mapveiw">
    <map
      id="myMap"
      style="width: 100%; height: 100%;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      show-location
      bindmarkertap="markertap"
      markers="{{markers}}"
    >
    <cover-view class="commodity_screen" bindtap="hideModal" wx:if="{{showModalStatus}}"></cover-view>
    <!--弹出框  -->
    <cover-view animation="{{animationData}}" class="commodity_attr_box" wx:if="{{showModalStatus}}">
 
      <cover-view class='placeBox'>
 
        <cover-view class='placeViewLt'>
          <cover-view class='viewTitle'>{{targetID.title}}</cover-view>
          <cover-view class='viewDis'> 100 </cover-view>
          <cover-view class='viewAddr'> 
            <image src="{{targetID.image[0]}}" mode="aspectFill" />
          </cover-view>
        </cover-view>
 
      </cover-view>
 
 
      <cover-view class='viewIcon'>
        <cover-image class='indexIcon2' src='/images/nav.png'></cover-image>
        <cover-view class='timeText1'>导航</cover-view>
      </cover-view>

    </cover-view>
  </map>
  

    <view class="bottom">
				<image src="../../images/another_add.png" bindtap="add" />
    </view>
</view>
